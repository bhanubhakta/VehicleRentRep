<AnchorPane id="AnchorPane" minHeight="122.0" prefHeight="700.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.RegistrationController">
  <children>
    <GridPane hgap="10.0" layoutX="119.0" layoutY="153.0" prefHeight="257.0" prefWidth="521.0" vgap="10.0">
      <children>
        <Label prefWidth="213.0" text="First Name" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <Label prefWidth="221.0" text="Last Name" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label prefWidth="247.0" text="Phone Number" GridPane.columnIndex="0" GridPane.rowIndex="2" />
